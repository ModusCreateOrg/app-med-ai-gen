{
  "name": "medical-reports-explainer",
  "version": "0.0.1",
  "description": "AI-powered medical report translator",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "dotenv -- npx cdk bootstrap",
    "deploy": "dotenv -- npx cdk deploy --require-approval never",
    "destroy": "dotenv -- cdk destroy -f",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "synth": "dotenv -- cdk synth",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cdk": "jest",
    "cdk": "dotenv -- cdk",
    "cdk:deploy": "dotenv -- npx cdk deploy",
    "cdk:destroy": "dotenv -- npx cdk destroy",
    "cdk:synth": "dotenv -- npx cdk synth"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
