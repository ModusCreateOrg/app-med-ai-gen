    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
