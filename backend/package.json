  "scripts": {
    "bootstrap": "cdk bootstrap",
    "build": "nest build",
    "deploy": "cdk deploy --require-approval never",
    "destroy": "cdk destroy -f",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
